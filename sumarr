package com.xyz;

public class sumarr {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] numbers = {3, 4, 5, -5, 0, 12};
		   int sum = 0;

		   // iterating through each element of the array 
		   for (int i=0;i<numbers.length;i++) {
		     sum += numbers[i];
		   }
		  
		   System.out.println("Sum = " + sum);
		 }
		}

